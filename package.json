{
  "name": "mernproject",
  "version": "1.0.0",
  "description": "test MERN stack",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "Oleksandr Iziumov <oleksandr.iziumov@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "10.4.5",
    "bcryptjs": "^2.4.3",
    "bootstrap-icons": "^1.9.1",
    "config": "^3.3.7",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.4",
    "prop-types": "^15.8.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19"
  }
}
